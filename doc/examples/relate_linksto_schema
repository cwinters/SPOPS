CREATE TABLE book (
   book_id      int not null,
   name         varchar(255) not null,
   primary key( book_id )
)

CREATE TABLE publisher (
   publisher_id int not null,
   name         varchar(255) not null,
   primary key( publisher_id )
)

CREATE TABLE publisher_book (
   publisher_id int not null,
   book_id      int not null,
   primary key( publisher_id, book_id )
)